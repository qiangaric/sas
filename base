/* sas变量引用必须是双引号*/
%let town = Chicago;

data work.city;
	input city $ state $;
	datalines;
 Detroit MI
 Chicago IL
 ;
run;

data work.city2 ;
	set work.city;
	if  city = "&town";
run;

proc print data=work.city2;
run;


/* 数据导入 */
PROC IMPORT DATAFILE="c:\sas\ego.csv" OUT=jeeshim.egov DBMS=CSV REPLACE;
   GETNAMES=YES;
   DATAROW=2;
RUN;
